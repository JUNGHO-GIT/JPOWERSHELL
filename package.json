{
  "name": "jpowershell",
  "displayName": "JPOWERSHELL",
  "description": "Powershell custom code",
  "publisher": "JUNGHO",
  "license": "Apache-2.0",
  "version": "1.3.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNGHO-GIT/JPOWERSHELL.git"
  },
  "scripts": {
    "sync": "bun .node/mjs/sync.mjs --bun --sync --server",
    "start": "bun .node/mjs/swc.mjs --bun --start --server",
    "build": "bun .node/mjs/swc.mjs --bun --build --server",
    "fix": "bun .node/mjs/fix.mjs --bun --fix",
    "reset": "bun .node/mjs/reset.mjs --bun --reset",
    "vsce": "bun .node/mjs/vsce.mjs --bun --package",
    "gcloud": "bun .node/mjs/gcloud.mjs --bun --server",
    "git-push-y": "bun .node/mjs/git.mjs --bun --push --y",
    "git-push-n": "bun .node/mjs/git.mjs --bun --push --n"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.4",
    "@swc/helpers": "0.5.17",
    "@types/eslint": "9.6.1",
    "@types/eslint__js": "9.14.0",
    "@types/node": "25.0.2",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "esbuild": "0.27.1",
    "eslint": "9.39.2",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-unicorn": "62.0.0",
    "@stylistic/eslint-plugin": "5.6.1",
    "globals": "16.5.0",
    "ts-morph": "27.0.2",
    "ts-prune": "0.10.3",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0"
  }
}
